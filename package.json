{
  "name": "Student",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "flow": "flow",
    "flow-stop": "flow stop",
    "lint": "eslint *.js **/*.js",
    "rename": "react-native-rename Student Manager -b org.studentmanager",
    "pretty": "prettier --semi false --print-width 100 --single-quote --trailing-comma all --write  \"src/**/*.js\"",
    "build": "cd android && ./gradlew assembleRelease",
    "testbuild": "react-native run-android --variant=release",
    "cleanup": "cd android && ./gradlew clean",
    "jetify": "npx jetify",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pod-install": "cd ios && pod install",
    "del-ios": "cd android/app/src/main/res && rm -rf raw drawable-hdpi drawable-mdpi drawable-xhdpi drawable-xxhdpi drawable-xxxhdpi",
    "del-android": "cd android/app/src/main/res && del raw drawable-hdpi drawable-mdpi drawable-xhdpi drawable-xxhdpi drawable-xxxhdpi",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
